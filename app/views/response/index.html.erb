<h1>Survey Response</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Compensation</th>
      <th>Experience</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @responses.each do |response| %>
      <tr>
        <td><%= response.title %></td>
        <td><%= response.compensation %></td>
        <td><%= response.experience %></td>
      </tr>
    <% end %>
  </tbody>
</table>
